<form action="/students" method="POST">
  <label>Student Full Name</label>
  <input name="full_name" placeholder=<%=@name_place_holder%> type="text"></input>
  <label>Year</label>
  <input
    name="year"
    placeholder=<%=@year_place_holder%>
    type="text"
  >
  </input>
  <label>Major</label>
  <input name="major" placeholder=<%=@major_place_holder%> type="text"></input>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <button type="submit">Submit!</button>
</form>